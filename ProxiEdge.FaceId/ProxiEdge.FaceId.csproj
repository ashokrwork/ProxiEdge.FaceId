<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Description>Microsoft Face Id wrapper library built using [WebClient] to enable progress reporting for Face Id API calls plus full encapsulation of the API requests entities and results into strongly typed objects for easier use.</Description>
    <Copyright>Copyright ProxiEDGE 2018. All rights reserved.</Copyright>
    <PackageProjectUrl>https://github.com/ashokrwork/ProxiEdge.FaceId</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ashokrwork/ProxiEdge.FaceId</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseUrl>https://licenses.nuget.org/MIT</PackageLicenseUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageTags>Cognitive,</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
  </ItemGroup>

</Project>
